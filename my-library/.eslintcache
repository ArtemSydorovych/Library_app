reload()\n    }\n  };\n\n  const editBook = (event) => {\n    let book = !jsData\n      ? null\n      : jsData.find((data) => {\n        return event.target.value === data.id.toString();\n        });\n    sessionStorage.setItem(\"edit\", JSON.stringify({\"title\": book.title ,\"id\": book.id}))\n\n    \n    if (book !== -1) {\n      localStorage.setItem(\"books\", JSON.stringify(jsData));\n      setState(0);\n    }\n  };\n\n  const readBook = (value) => {\n    let book = !jsData\n      ? null\n      : jsData.find((data) => {\n          return value === data.title;\n        });\n    sessionStorage.setItem(\"read\", JSON.stringify({\"title\": book.title ,\"id\": book.id}))\n  };\n\n  const listItems = !jsData\n    ? null\n    : jsData.map((data) => {\n        return (\n          <div className=\"book-list\">\n            <img width=\"300px\" src={data.url} alt={data.title}></img>\n            <ListGroup>\n              <ListGroup.Item>\n                <b>Title:</b> {data.title}\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <b>Author:</b> {data.author}\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <b>Publishment year:</b> {data.year}\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <b>Genre:</b> {data.genre}\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Link\n                  to={\"/\" + data.title}\n                  onClick={() => readBook(data.title)}\n                  value={data.title}\n                  className=\"btn\"\n                >\n                  <Button>Read</Button>\n                </Link>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Button className=\"btn\" onClick={deleteBook} value={data.id}>\n                  Delete\n                </Button>\n                <Link\n                  to={\"/edit/\" + data.title}\n                  className=\"btn\"\n                >\n                  <Button\n                  value={data.id}\n                  name={data.title}\n                  onClick={editBook}\n                  >\n                    Edit\n                  </Button>\n                </Link>\n              </ListGroup.Item>\n            </ListGroup>\n          </div>\n        );\n      });\n  return <div>{listItems}</div>;\n};\n","/Users/artemsydorovych/Desktop/Programming/Library_app/my-library/src/componetns/EditBook/EditBook.js",[],"/Users/artemsydorovych/Desktop/Programming/Library_app/my-library/src/componetns/EditBook/index.js",[],{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","severity":1,"message":"82","line":9,"column":12,"nodeType":"83","messageId":"84","endLine":9,"endColumn":17},{"ruleId":"85","severity":1,"message":"86","line":22,"column":40,"nodeType":"87","messageId":"88","endLine":22,"endColumn":42},{"ruleId":"85","severity":1,"message":"86","line":35,"column":40,"nodeType":"87","messageId":"88","endLine":35,"endColumn":42},{"ruleId":"77","replacedBy":"89"},{"ruleId":"79","replacedBy":"90"},{"ruleId":"81","severity":1,"message":"82","line":9,"column":10,"nodeType":"83","messageId":"84","endLine":9,"endColumn":15},"no-native-reassign",["91"],"no-negated-in-lhs",["92"],"no-unused-vars","'state' is assigned a value but never used.","Identifier","unusedVar","eqeqeq","Expected '===' and instead saw '=='.","BinaryExpression","unexpected",["91"],["92"],"no-global-assign","no-unsafe-negation"]